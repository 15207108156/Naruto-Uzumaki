	<select id="selectByNearby" resultMap="BaseResultMap">
		SELECT
		tt.*,ABS(tt.`Longitude` -
		#{lng,jdbcType=DOUBLE}) + ABS(tt.`Latitude` - #{lat,jdbcType=DOUBLE}) lnglat
		FROM TrainTeam tt
		ORDER BY lnglat ASC
	</select>

