				</if>
				<if test="selectType == 1">
					v.VenueNO LIKE concat('%',concat('${keyword}','%'))
				</if>
				<if test="selectType == 2">
					vl.Content LIKE concat('%',concat('${keyword}','%'))
				</if>
			</if>
		</where>
	</select>
	
