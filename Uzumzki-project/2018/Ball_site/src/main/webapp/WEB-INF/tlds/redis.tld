<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<taglib xmlns="http://java.sun.com/xml/ns/j2ee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	version="2.0"
	xsi:schemaLocation="http://java.sun.com/xml/ns/j2ee http://java.sun.com/xml/ns/j2ee/web-jsptaglibrary_2_0.xsd">

	<display-name>Redis Tags</display-name>
	<tlib-version>1.0</tlib-version>
	<short-name>redis</short-name>
	<uri>/redis-tags</uri>
	<tag>
		<name>top</name>
		<tag-class>com.xiaoyi.ssm.template.jsp.TopTag</tag-class>
		<body-content>scriptless</body-content>
		<attribute>
			<name>topId</name>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<name>var</name>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
	</tag>
	<tag>
		<name>estate</name>
		<tag-class>com.xiaoyi.ssm.template.jsp.EstateTag</tag-class>
		<body-content>scriptless</body-content>
		<attribute>
			<name>estateId</name>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<name>var</name>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
	</tag>
	
	<tag>
		<name>web</name>
		<tag-class>com.xiaoyi.ssm.template.jsp.WebTag</tag-class>
		<body-content>scriptless</body-content>
		<attribute>
			<name>webId</name>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<name>var</name>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
	</tag>
	<!-- <redis:estate var="estate" estateId="110">${estate}</redis:estate> -->
</taglib>